{
    "name": "tslib-starter",
    "version": "0.0.1",
    "license": "MIT",
    "description": "Node typescript package starter.",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "!**/*.spec.*",
        "!**/*.json",
        "CHANGELOG.md",
        "LICENSE",
        "README.md"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "rimraf dist && tsc",
        "test": "jest",
        "cov": "jest --coverage",
        "lint": "eslint src --ext .ts",
        "format": "prettier \"src/**/*.ts\" --write",
        "check": "run-s format lint"
    },
    "repository": {
        "url": "https://github.com/michaelwooley/tslib-starter",
        "type": "git"
    },
    "author": {
        "name": "Michael Wooley",
        "email": "wm.wooley@gmail.com",
        "url": "https://github.com/michaelwooley"
    },
    "keywords": [
        "typescript",
        "starter",
        "template",
        "boilerplate",
        "library",
        "node",
        "typedoc",
        "jest",
        "nyc",
        "eslint",
        "prettier"
    ],
    "homepage": "https://github.com/michaelwooley/tslib-starter",
    "readme": "README.md",
    "preview": true,
    "bugs": {
        "url": "https://github.com/michaelwooley/tslib-starter/issues"
    },
    "devDependencies": {
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-functional": "^4.4.1",
        "eslint-plugin-import": "^2.27.2",
        "jest": "^29.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.2",
        "rimraf": "^3.0.2",
        "standard-version": "^9.5.0",
        "ts-jest": "^29.0.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "prettier": {
        "singleQuote": true,
        "tabWidth": 4,
        "semi": true,
        "trailingComma": "es5",
        "useTabs": false
    }
}
